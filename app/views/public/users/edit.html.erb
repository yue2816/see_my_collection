<h1>ユーザー編集ページ</h1>

<%= form_with model: @user do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>名前</h4>
  <%= f.text_field :name %>

  <h4>自己紹介文</h4>
  <%= f.text_area :introduction %>

  <h4>コレクションしている物</h4>
  <%= f.text_area :my_collection %>

  <%= f.submit '編集を保存' %>
<% end %>